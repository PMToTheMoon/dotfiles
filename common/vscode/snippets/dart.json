{
	"Stateless Widget": {
		"prefix": [
			"stateless",
		],
		"body": [
			"import 'package:flutter/material.dart';",
			"",
			"class $Name extends StatelessWidget {",
			"\tconst $Name({",
			"\tKey? key,",
			"}) : super(key: key);",
			"",
			"\t@override",
			"\tWidget build(BuildContext context) {",
			"\t\treturn ${0:const Text('$Name')};",
			"\t}",
			"}",
		],
		"description": "A Flutter Stateless Widget."
	},
	"Stateful Widget": {
		"prefix": [
			"stateful"
		],
		"body": [
			"import 'package:flutter/material.dart';",
			"",
			"class $Name extends StatefulWidget {",
			"\tconst $Name({",
			"\t\tKey? key,",
			"\t}) : super(key: key);",
			"",
			"\t@override",
			"\t_${Name}State createState() => _${Name}State();",
			"}",
			"",
			"class _${Name}State extends State<${Name}> {",
			"\t_${Name}State();",
			"",
			"\t@override",
			"\tWidget build(BuildContext context) {",
			"\t\treturn ${0:const Text('$Name')};",
			"\t}",
			"}",
		]
	},
	"Freezed Class": {
		"prefix": [
			"freezed",
		],
		"body": [
			"import 'package:freezed_annotation/freezed_annotation.dart';",
			"",
			"part '${1:file_name}.freezed.dart';",
			"",
			"@freezed",
			"class $Class with _$$Class {",
			"\tconst $Class._();",
			"",
			"\tconst factory $Class({",
			"\t\t$0,",
			"\t}) = _$Class;",
			"}",
		],
		"description": "A Flutter Stateless Widget."
	},
	"Flutter base main": {
		"prefix": [
			"boostrap main"
		],
		"body": [
			"import 'package:flutter/material.dart';",
			"",
			"void main() {",
			"  runApp(const MyApp());",
			"}",
			"",
			"class MyApp extends StatelessWidget {",
			"\tconst MyApp({Key? key}) : super(key: key);",
			"",
			"\t@override",
			"\tWidget build(BuildContext context) {",
			"\t\treturn MaterialApp(",
			"\t\t\ttitle: 'Firebase demo',",
			"\t\t\ttheme: ThemeData(",
			"\t\t\t\tprimarySwatch: Colors.blueGrey,",
			"\t\t\t),",
			"\t\t\thome: const HomePage(),",
			"\t\t);",
			"\t}",
			"}",
			"",
			"class HomePage extends StatelessWidget {",
			"\tconst HomePage({",
			"\t\tKey? key,",
			"\t}) : super(key: key);",
			"",
			"\t@override",
			"\tWidget build(BuildContext context) {",
			"\t\treturn Scaffold(",
			"\t\t\tappBar: AppBar(",
			"\t\t\t\ttitle: const Text('Home'),",
			"\t\t\t),",
			"\t\t\tbody: const Center(",
			"\t\t\t\tchild: Text('Home'),",
			"\t\t\t),",
			"\t\t);",
			"\t}",
			"}",
		]
	},
	"Neat dimensions": {
		"prefix": [
			"bootsrap neat"
		],
		"body": [
			"import 'package:neat/generator.dart';",
			"",
			"part 'dimensions.nt.dart';",
			"",
			"@Neat.generate",
			"class Dimensions {",
			"//spaces",
			"\tstatic const double space5 = 5;",
			"\tstatic const double space4 = 8;",
			"\tstatic const double space3 = 13;",
			"\tstatic const double space2 = 21;",
			"\tstatic const double space1 = 34;",
			"",
			"\t//paddings",
			"\tstatic const double padding5 = space5;",
			"\tstatic const double padding4 = space4;",
			"\tstatic const double padding3 = space3;",
			"\tstatic const double padding2 = space2;",
			"\tstatic const double padding1 = space1;",
			"",
			"\t//borders",
			"\tstatic const double borderRadius1 = 13;",
			"\tstatic const double borderSize = 2;",
			"",
			"\t//icons",
			"\tstatic const double icon1 = 34;",
			"}",
		]
	}
}